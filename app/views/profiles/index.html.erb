<h1>Listing profiles</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>A1rate</th>
    <th>A2rate</th>
    <th>A3rate</th>
    <th>H1rate</th>
    <th>H2rate</th>
    <th>H3rate</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%= profile.name %></td>
    <td><%= profile.description %></td>
    <td><%= profile.a1rate %></td>
    <td><%= profile.a2rate %></td>
    <td><%= profile.a3rate %></td>
    <td><%= profile.h1rate %></td>
    <td><%= profile.h2rate %></td>
    <td><%= profile.h3rate %></td>
    <td><%= link_to 'Show', profile %></td>
    <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
    <td><%= link_to 'Destroy', profile, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Profile', new_profile_path %>
